/*重定义*/
input,
view {
  box-sizing: border-box;
}
image {
  vertical-align: middle;
  width: 100%;
  height: 0;
  display: inline-block;
}
image[mode='aspectFit'] {
  height: 100%;
}
image[mode='aspectFill'] {
  height: 100%;
}
button {
  vertical-align: middle;
  font-size: 28rpx !important;
}
button.radiuBtn {
  border-radius: 60rpx;
}
button.noradius {
