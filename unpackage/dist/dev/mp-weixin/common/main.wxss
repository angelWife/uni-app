/*个人新增*/
.float-left {
  float: left;
}
.float-right {
  float: right;
}
.text-right {
  text-align: right;
}
.text-left {
  text-align: left;
}
.text-center {
  text-align: center;
}
.clearfix {
  clear: both;
}
.clearfix:after {
  display: block;
  clear: both;
  content: "";
}
.border-box {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
}
.breakWord {
  overflow: hidden;
  white-space: normal;
  word-break: break-all;
  word-wrap: break-word;
}
.modalmask {
  height: 100%;
  width: 100%;
  overflow: hidden;
  position: fixed;
  background: rgba(0, 0, 0, 0.2);
  top: 0;
  left: 0;
  z-index: 20;
}
.modalmask .centerModal {
  width: 80%;
  margin: 0 auto;
  background: #fff;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
}
.modalmask .centerModal .title {
  line-height: 80rpx;
  padding: 0 30rpx;
  position: relative;
  border-bottom: 1px solid #e6e6e6;
}
.modalmask .centerModal .title .close {
  position: absolute;
  top: 0;
  right: 0;
  height: 80rpx;
  width: 80rpx;
  text-align: center;
  color: #999;
}
.modalmask .centerModal .center {
  padding: 20rpx 30rpx;
  text-align: center;
}
.modalmask .centerModal .foot {
  border-top: 1px solid #e6e6e6;
}
.modalmask .centerModal .foot > view {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.modalmask .centerModal .foot .sure {
  color: #007AFF;
}
.modalmask .centerModal .foot .cancel {
  color: #999;
  border-right: 1px solid #e6e6e6;
}
.comModal {
  position: absolute;
  width: 100%;
  background: #fff;
  left: 0;
  bottom: 0;
}
.comfootBtn {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  background: #b40000;
  font-size: 30rpx;
  letter-spacing: 5px;
  padding-bottom: constant(safe-area-inset-bottom);
}
.comNumber {
  border: 1px solid #ccc;
  border-radius: 0.3em;
  height: 74rpx;
  line-height: 70rpx;
  overflow: hidden;
}
.comNumber input {
  float: left;
  width: 100rpx;
  line-height: 70rpx;
  height: 70rpx;
  background: #fff;
  border: none;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  text-align: center;
}
.comNumber .iconfont {
  float: left;
  background: #efefef;
  width: 70rpx;
  line-height: 70rpx;
  height: 70rpx;
  text-align: center;
  color: #fff;
}
.comNumber .iconfont.touch {
  background: #ccc;
}
.comHeadPic {
  position: relative;
  border-radius: 100%;
}
.comHeadPic .headPic {
  border-radius: 100%;
  height: 100%;
}
.comHeadPic .grade {
  position: absolute;
  width: calc(100% + 30rpx) !important;
  top: -10rpx;
  left: -15rpx;
}
.comNews {
  background: #fff;
}
.comNews .listBox {
  position: relative;
  height: 140rpx;
}
.comNews .listBox .btn {
  height: 100%;
  width: 140rpx;
  text-align: center;
  line-height: 140rpx;
  color: #fff;
}
.comNews .listBox .look {
  background: #4c8ff7;
}
.comNews .listBox .delete {
  background: #b40000;
}
.comNews .newBox {
  position: absolute;
  height: 140rpx;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 2;
  background: #fff;
  -webkit-transition: left 0.3s ease;
  transition: left 0.3s ease;
}
.comNews .newBox .pic {
  width: 100rpx;
  height: 100rpx;
  overflow: hidden;
  border-radius: 100rpx;
  margin: 0 40rpx;
}
.comNews .newBox .main {
  height: 140rpx;
  border-bottom: 1px solid #ccc;
  padding: 24rpx 0;
}
.comNews .newBox .main .text {
  margin-top: 10rpx;
}
.comListBox {
  padding: 0 24rpx;
  background: #fff;
}
.comListBox .item {
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #ccc;
}
.comListBox .item:last-child {
  border: 0;
}
.comItem .pic {
  margin: 0 24rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  overflow: hidden;
}
.comItem .main {
  padding: 20rpx 0;
  border-bottom: 1px solid #ccc;
}
.comItem .right-pic {
  height: 80rpx;
  line-height: 80rpx;
}
.comResult {
  padding: 70rpx 0;
  text-align: center;
}
.comResult .pic {
  width: 180rpx;
  max-width: 90px;
  margin: 0 auto;
}
.comResult .text {
  margin-top: 20rpx;
}
icon.remove {
  position: absolute;
