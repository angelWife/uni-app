/*自定义按钮*/
button[type="red"] {
  background: #b40000 !important;
  color: #fff !important;
}
button[type="red"]::after {
  border: none !important;
  box-shadow: none !important;
}
button[type="default"] {
  color: #999 !important;
}
button[type="rednull"] {
  background: none !important;
  color: #b40000 !important;
  border: 1px solid #b40000;
}
button[type="rednull"]::after {
  border: none !important;
}
button[type="text"] {
  background: none !important;
  color: #444;
  border: none;
}
button[type="text"]::after {
  border: none !important;
  box-shadow: none !important;
}
button[type="share"] {
  background: none !important;
  border: none;
  color: inherit;
}
button[type="share"]::after {
  border: none !important;
  box-shadow: none !important;
}
button[type="share"].red {
  color: #B40000;
}
button[type="null"] {
  background: none !important;
  color: #666 !important;
  border: 1px solid #ccc;
  box-shadow: 50%;
}
button[type="null"]::after {
  border: none !important;
  box-shadow: none !important;
}
button[type="orange"] {
  background: #ff9d19 !important;
  color: #fff !important;
  border: none;
}
button[type="orange"]::after {
  border: none !important;
  box-shadow: none !important;
}
button[type="blue"] {
  background: #4c8ff7 !important;
  color: #fff !important;
  border: none;
}
button[type="blue"]::after {
  border: none !important;
  box-shadow: none !important;
}
button[size="mini"] {
  font-size: 26rpx !important;
  line-height: 72rpx;
}
button[size="mini"]::after {
  box-shadow: none;
  border: 0;
}
