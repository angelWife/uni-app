<view class="content"><view class="modal listBox"><view data-event-opts="{{[['tap',[['linkTo',['activityLevel',2]]]]]}}" class="item flex" bindtap="__e"><view class="name"><image src="../../static/images/starticke.png" mode="widthFix"></image><text>星票</text></view><view class="nums text-right flex-1">{{''+(totals.total?totals.total:0)+''}}</view><view class="iconfont icon-right"></view></view><view data-event-opts="{{[['tap',[['linkTo',['bankCards']]]]]}}" class="item flex" bindtap="__e"><view class="name"><image src="../../static/images/icon-bankcard.png" mode="widthFix"></image><text>银行卡</text></view><view class="nums text-right flex-1"></view><view class="iconfont icon-right"></view></view></view><view class="modal prodList"><view class="navBox flex"><block wx:for="{{nav}}" wx:for-item="item" wx:for-index="ind" wx:key="ind"><view data-event-opts="{{[['tap',[['changeNav',[ind,'$0'],[[['nav','',ind,'key']]]]]]]}}" class="{{['item flex-1',(item.choose)?'active':'']}}" bindtap="__e"><text>{{item.val}}</text></view></block></view><view class="productContent clearfix"><scroll-view class="myscroll" scroll-y="true" data-event-opts="{{[['scrolltolower',[['loadMore',['$event']]]]]}}" bindscrolltolower="__e"><view class="clearfix"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item flex item-center"><view data-event-opts="{{[['tap',[['gotoDetail',['$0'],[[['dataList','',index]]]]]]]}}" class="item-box" bindtap="__e"><view class="pic"><image src="{{item.imgPath}}" mode="widthFix"></image></view><view class="prd_msg"><view class="name clamp clamp-1">{{item.name}}</view><view class="price flex just-con-b"><view class="red"><text class="fs10">¥</text><text class="fs16">{{item.priceSale}}</text><text class="c999">{{'('+item.priceSale*10+'星票)'}}</text></view></view><view class="fs10 c999">{{'已兑'+item.totalNums+''}}</view></view></view></view></block></view><block wx:if="{{nodata}}"><view class="noData flex f-row just-con-c item-center"><view class="text-center"><image src="/static/images/nodata.png" mode="widthFix"></image><view class="text-center c666 fs16">这里还没有内容</view></view></view></block><block wx:if="{{nomore}}"><view class="noMore">~已经到底了！~</view></block></scroll-view></view></view></view>