.result {
  padding: 70rpx 0;
  text-align: center;
}
.result .pic {
  width: 180rpx;
  max-width: 90px;
  margin: 0 auto;
}
.result .text {
  margin-top: 20rpx;
}
.btnBox {
  padding: 0 24rpx;
}
.btnBox .btn1 {
  width: 100%;
  border-radius: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  margin-top: 30rpx;
  background: -webkit-gradient(linear, left top, left bottom, from(#db0b00), to(#fd5959));
  background: -o-linear-gradient(top, #db0b00 0%, #fd5959 100%);
  background: linear-gradient(180deg, #db0b00 0%, #fd5959 100%);
}
.btnBox button {
  margin-top: 30rpx;
  line-height: 80rpx;
  border-radius: 80rpx;
  width: 100%;
}
.btnBox navigator {
  display: block;
  text-align: center;
  margin-top: 30rpx;
}

